{
  "name": "maps-scraper",
  "version": "1.0.0",
  "description": "Google Maps scraper for stays in Varkala, Kerala - stores in Neon PostgreSQL",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "pnpm build && node dist/index.js",
    "dev": "tsx src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "scraper",
    "google-maps",
    "playwright",
    "drizzle",
    "neon"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.0",
    "pino": "^10.3.1",
    "playwright": "^1.49.0",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "drizzle-kit": "^0.28.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
